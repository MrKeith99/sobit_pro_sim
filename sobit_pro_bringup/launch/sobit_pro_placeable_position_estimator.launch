<?xml version="1.0"?>
<launch>

	<group ns = "placeable_position_estimator">

		<param name="execute_default" type="bool" value="false"/>
		<param name="pub_plane_cloud" type="bool" value="true"/>

		<param name="sub_point_topic_name" type="str" value="/points2"/>
		<param name="sub_ctrl_topic_name" type="str" value="execute_ctrl"/>

		<param name="base_frame_name" type="str" value="/base_footprint"/>
		<param name="camera_frame_name" type="str" value="/head_depth_camera_link"/>
		<param name="map_frame_name" type="str" value="/odom"/> <!--map-->

		<param name="depth_range_min_x" type="double" value="0.1"/>
		<param name="depth_range_max_x" type="double" value="2.0"/>
		<param name="depth_range_min_z" type="double" value="0.1"/>
		<param name="depth_range_max_z" type="double" value="1.2"/> <!--1.6-->

		<node pkg="placeable_position_estimator" type="placeable_position_estimator" name="placeable_position_estimater_node" output="screen"/>

	</group>

</launch>
